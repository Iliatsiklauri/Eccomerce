{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "concurrently \"tsc -w\" \"nodemon ./dist/server.js\"",
    "start-server": "pg_ctl start -D \"C:\\Program Files\\PostgreSQL\\16\\data\"",
    "stop-server": "pg_ctl stop -D \"C:\\Program Files\\PostgreSQL\\16\\data\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create -- -d src/db/database-connect.ts src/db/migrations",
    "migration:generate": "npm run typeorm migration:generate -- -d src/db/database-connect.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/db/database-connect.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/db/database-connect.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.651.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "clear": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-joi-validation": "^5.0.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.3",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.14.0"
  }
}
